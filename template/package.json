    "es6-promise": "^4.2.4",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.7",
    "url-loader": "^1.0.1",
    {{#less}}
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    {{/less}}
    {{#htmlwebpackPlugin}}
